#include
using namespace std;
int main()
{
int arr[9];
cout<<"Enter the elements of array\n";
for(int i=0;i<8;i++)
{
cin>>arr[i];
}
cout<<"The elements of your array are:\n";
for(int i=0;i<8;i++)
{
cout<<arr[i];
}
cout<<"\nDeleting Index...\n";
for(int i=9;i>0;i++)
{
arr[i]=arr[i-1];
}
cout<<"\nIndex deleted at the start sucessfully:\n";
for (int i=0;i<9;i++)
{
cout<<arr[i];
}
return 0;
}
